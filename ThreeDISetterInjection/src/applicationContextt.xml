<?xml version="1.0" encoding="UTF-8"?>  
<beans  
    xmlns="http://www.springframework.org/schema/beans"  
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"  
    xmlns:p="http://www.springframework.org/schema/p"  
    xsi:schemaLocation="http://www.springframework.org/schema/beans   
http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">  
  
  <bean id="address1" class="com.udemy.Address">
  <property name="city" value="piscataway"></property>
  <property name="state" value="New Jersey"></property>
  <property name="country" value="USA"></property>
  
  </bean>
  <bean id="employee1" class="com.udemy.Employee">
  <property name="id" value="23432"></property>
  <property name="name" value="Prasad"></property>
  <property name="address" ref="address1"></property>  </bean>
  
  
  </beans>